{
	"name": "benmanu/silverstripe-leafletfield",
	"description": "Provides a form field type allowing users to draw features on a map, the features are stored as geoJSON objects in a single database field",
	"type": "silverstripe-vendormodule",
	"homepage": "https://github.com/benmanu/silverstripe-leafletfield",
	"keywords": ["silverstripe", "leaflet", "map"],
	"support": {
		"issues": "https://github.com/benmanu/silverstripe-leafletfield/issues"
	},
	"authors": [
		{
			"name": "benmanu",
			"email": "ben@silverstripe.com"
		}
	],
	"require": {
		"silverstripe/vendor-plugin": "^1.0",
		"silverstripe/cms": "^4.0"
	},
	"autoload": {
		"psr-4": {
			"BenManu\\LeafletField\\": "src/"
		}
	},
	"extra": {
		"installer-name": "leafletfield",
		"expose": [
			"client"
		]
	}
}
